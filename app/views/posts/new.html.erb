<div class="container">
<h1>Post New Page</h1>
<%= simple_form_for([@city,@community,@post], html: { class: "new-form" }) do |f|%>
  <%= f.input :title %>
  <%= f.input :content %>
  <%= f.input :website_url %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, "Add post", class: "btn btn-primary w-100 new-form-submit"%>
<% end %>
</div>
